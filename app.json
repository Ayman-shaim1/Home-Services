{
  "expo": {
    "main": "App.js",
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Autorisez $(PRODUCT_NAME) Ã  utiliser votre emplacement."
        }
      ]
    ],
    "name": "HomeService",
    "slug": "HomeService",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.helloworld.homeservice"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.helloworld.homeservice"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
